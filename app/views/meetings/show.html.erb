<div class="page-header">
  <h1>Meeting #<%= @meeting.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Held on</dt>
  <dd><%= @meeting.held_on %></dd>

  <dt>Course</dt>
  <dd><%= @course.department %> <%= @course.number %> <%= @course.title %></dd>

<h2>Students who attended</h2>
  <% @students.each do|studentname|  %>
 <ul>
 	<li>	
	<%= studentname.first_name %> <%= studentname.last_name %>
	</li>
</ul>  
  <% end %>

 



</dl>
